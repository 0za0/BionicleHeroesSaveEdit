<Application x:Class="HeroSaveEditDebugging.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HeroSaveEditDebugging"
             xmlns:conv="clr-namespace:WpfHexaEditor.Core.Converters;assembly=WPFHexaEditor"

             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <conv:ByteToHexStringConverter x:Key="ByteHexStringConverter" Show0xTag="False" />
        <conv:LongToHexStringConverter x:Key="LongHexStringConverter" />
    </Application.Resources>
</Application>
